{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/assets/(.*\\.js)",
      "dest": "/dist/assets/$1",
      "headers": {
        "Content-Type": "application/javascript; charset=utf-8"
      }
    }
  ]
}
